n=int(input("enter number you want to check:"))
if n==1:
    print("it is not prime number!")
elif n>1:
    for i in range(2,n):
        if n%i==0:
            print("it is not prime number!!")
            break
        else:
             print("it is prime number.")
             break
else:
    print("it is not prime numner!!!")
